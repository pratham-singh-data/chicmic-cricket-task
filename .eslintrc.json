{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["error", 4],
        "object-curly-spacing": ["error", "always"],
        "comma-dangle": ["error", {"functions": "never", "objects": "always", "arrays": "always"}]
    }
}
